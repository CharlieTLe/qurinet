# filter what we send upstream
domain-needed
bogus-priv
filterwin2k

# allow /etc/hosts and dhcp lookups via *.lan
local=/local/
address=/local/10.0.0.0

dhcp-option=6, 169.237.92.4, 169.237.1.250, 169.237.62.3, 169.237.92.5, 169.237.167.3, 169.237.62.7, 169.237.250.250
dhcp-leasefile=/tmp/dhcp.leases
interface=br0
bind-interfaces
